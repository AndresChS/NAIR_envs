CmaOptimizer {
	signature_prefix = DATE_TIME
	min_progress= 1e-4
	
	SimulationObjective {
		max_duration = 10
		
		ModelHyfydy {
			model_file = models/H0918_KneeExoV0.hfd
			state_init_file = init_states/InitStateExoGait_0.zml#InitStateExo_Flex_v0.zml #InitStateExoGait_0.zml
