			minimize = true
			#EffortMeasure {measure_type = Wang2012 weight = 0.1 minimize = true}	
			<< measures/MeasureGait10.scone >>
			#ScriptMeasure {script_file = measure.lua weight = 1 minimize = true}
		}
		
	}
	
