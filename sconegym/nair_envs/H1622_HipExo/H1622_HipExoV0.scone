# Model used in simulation
ModelHyfydy {
	model_file = models/H1622_HipExoV0.hfd

	# Set initial state
	state_init_file = init_state/InitStateExo_Flex_v0.zml #Gait10.zml
	fixed_control_step_size = 0.005
	initial_equilibration_activation = 0.05
	zero_velocity_equilibration = 1
	use_omnidirectional_root_dofs = 1
	<< neural_delays/neural_delays_FEA_v4.zml >>
}
